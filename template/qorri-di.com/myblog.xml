  height: 100vh;
}

@keyframes gradient {
  0% {
    background-position: 0% 50%;
  }
  50% {
    background-position: 100% 50%;
  }
  100% {
    background-position: 0% 50%;
  }
}
/*Header and Navbar*/
.header,.navbar-top {
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: transparent;
    position: fixed;
    width: 100%;
    top: 0;
    /*box-shadow: 0 px 8px rgba(0, 0, 0, 0.1);*/
    /*z-index: 1000;*/
}
.btn-start,.btn-etc,.btn-etc-toggle{
    background-color: white;
    padding: 10px 10px;
}
.btn-start {
    border-bottom-right-radius: 25%;
    text-align: left;
}
.btn-start-toggle{
    position: absolute;
    top: 2.7rem;
    width: 200px;
    min-height: 100px;
    background: #fff;
    box-shadow: 0 8px 32px 0 rgba(0, 0, 0, 0.1);
    /*border: 1px solid rgba(255, 255, 255, 0.18);*/
    visibility: hidden;
}
.btn-start-toggle.active{
    opacity: 1;
    visibility: visible;
}
.btn-etc {
    border-bottom-left-radius: 25%;
    text-align: right;
}
.btn-start a, .btn-etc a{
    width: 100%;
    text-decoration: none;
}
.btn-start a:hover, .btn-etc a:hover{
    /*background-color: red;*/
    /*width: 100%;*/
    /*height: 100%;*/
}
.footer{
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: transparent;
    position: fixed;
    width: 100%;
    bottom: 0;
}
.navbar-bottom{
    height: 4rem;
    padding: 0 1rem;
    background: rgba(255, 255, 255, 0.1);
    box-shadow: 0 8px 32px 0 rgba(31, 38, 135, 0.37);
    backdrop-filter: blur(1px);
    -webkit-backdrop-filter: blur(4px);
    border: 1px solid rgba(255, 255, 255, 0.18);
    display: flex;
    justify-content: center;
    border-radius: 1rem;
    margin-bottom: 1rem;
}
.btn-center{
    --scale: 1;
    list-style: none;
    margin: 0;
    padding: 0;
    display: flex;
    gap: 1rem;
    justify-content: center;
    align-items: center;
}
.btn-center img{
    height: calc(3rem * var(--scale));
    width: calc(3rem * var(--scale));
    position: relative;
    top: calc(3rem * var(--scale) - 3rem);
    transition: 250ms all ease-in-out;
}
.btn-center img.loading{
    animation: 1s loading ease-in infinite;
}
.btn-center img:hover{
    transform: translateY(-1rem);
}
.btn-etc-toggle ul,.btn-center ul{
    /*display: flex;*/
    /*list-style: none;*/
}
